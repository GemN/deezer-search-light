{
  "presets": ["react", "flow",
    ["env", {
      "targets": {
        "ie": "9"
      }
    }]
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-es2015-classes", {
      "loose": true
    }]
  ]
}
